# Configurações da Aplicação
# Copie este arquivo para .env e preencha com seus valores reais

# Flask
SECRET_KEY=your-secret-key-here
FLASK_ENV=development

# Credenciais LECOM
LECOM_USER=seu_usuario_lecom
LECOM_PASS=sua_senha_lecom

# API Mistral (para OCR)
MISTRAL_API_KEY=sua_chave_api_mistral

# Banco de Dados
DATABASE_URL=sqlite:///naturalizacao.db

# Configurações de Upload
MAX_UPLOAD_SIZE=16777216
ALLOWED_EXTENSIONS=xlsx,xls,csv

# Configurações de Selenium
SELENIUM_TIMEOUT=40
SELENIUM_HEADLESS=false

# Configurações de Logs
LOG_LEVEL=INFO
